{
    "name": "@phase21/logos",
    "version": "2.7.0",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "optimize-chain-logos": "rimraf ./optimized/chain & svgo -q -p 8 -f ./raw/chains -o ./optimized/chain",
        "optimize-symbol-logos": "rimraf ./optimized/symbol & svgo -q -p 8 -f ./raw/symbols -o ./optimized/symbol",
        "optimize-api-provider-logos": "rimraf ./optimized/api-provider & svgo -q -p 8 -f ./raw/api-providers -o ./optimized/api-provider",
        "build": "pnpm run optimize-chain-logos && pnpm run optimize-symbol-logos && pnpm run optimize-api-provider-logos && node scripts/build.js",
        "build-svg": "pnpm run optimize-chain-logos && pnpm run optimize-symbol-logos && pnpm run optimize-api-provider-logos && node scripts/build-svg.js",
        "fetch": "node scripts/fetch-missing.js",
        "package": "pnpm run build-svg && yarn pack --filename logos.tgz",
        "prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "release": "pnpm changeset publish"
    },
    "dependencies": {
        "camelcase": "^6.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.7",
        "@babel/preset-react": "^7.23.3",
        "@changesets/changelog-github": "^0.5.0",
        "@changesets/cli": "^2.27.1",
        "@phase21/api-integrations": "^1.1.0",
        "@phase21/chains": "^0.5.0",
        "dropbox": "^10.34.0",
        "node-fetch": "^2.6.1",
        "dotenv": "^16.4.1",
        "prettier": "^3.2.4",
        "rimraf": "^5.0.5",
        "svgo": "^3.2.0"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "files": [
        "dist"
    ]
}