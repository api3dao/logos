{
    "name": "beta-logos",
    "version": "0.0.3",
    "license": "MIT",
    "sideEffects": false,
    "scripts": {
        "optimize-chain-logos": "rimraf ./optimized/chains & svgo -q -p 8 -f ./raw/chains -o ./optimized/chains",
        "optimize-symbol-logos": "rimraf ./optimized/symbols & svgo -q -p 8 -f ./raw/symbols -o ./optimized/symbols",
        "optimize-api-provider-logos": "rimraf ./optimized/api-providers & svgo -q -p 8 -f ./raw/api-providers -o ./optimized/api-providers",
        "build": "yarn optimize-chain-logos && yarn optimize-symbol-logos && yarn optimize-api-provider-logos && node scripts/build.js",
        "package": "yarn build && yarn pack --filename logos.tgz",
        "prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "release": "yarn changeset publish"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./dist/svg/chains/*.svg": "./dist/svg/chains/*.svg",
        "./dist/svg/symbols/*.svg": "./dist/svg/symbols/*.svg"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-react": "^7.14.5",
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.2",
        "@svgr/cli": "^5.5.0",
        "@svgr/core": "^5.5.0",
        "@types/react": "^18.2.39",
        "camelcase": "^6.2.0",
        "prettier": "^3.1.0",
        "rimraf": "^3.0.2",
        "svgo": "^2.5.0",
        "terser": "^5.7.2"
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "files": [
        "dist"
    ],
    "dependencies": {
        "@api3/chains": "^4.1.0"
    }
}